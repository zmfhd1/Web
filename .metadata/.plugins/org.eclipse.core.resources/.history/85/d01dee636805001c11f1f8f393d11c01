<script src="jquery-3.2.1.min.js"></script>
<script>
$(document).ready(function() {
	$('img').attr("src", "/htmltest/images/iphone.png")
	var src = $("img").attr("src"); //getter - img태그의 src속성값
	$("#result").html(src); 
});
</script>
